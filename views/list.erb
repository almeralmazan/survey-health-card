<div id="list-view">

  <h3>List of person</h3>

  <div id="table-list">
  
  </div><!-- end table-list -->

  <table border="1">
    <tr>
      <th rowspan="2">Region</th>
      <th rowspan="2">Province</th>
      <th colspan="2">Gender</th>
      <th rowspan="2">Age range</th>
      <th colspan="2">Nature Of Employment</th>
      <th colspan="2">Existing HMO</th>
    </tr>
    <tr>
      <td>Male</td>
      <td>Female</td>
      <td>Contractual</td>
      <td>Permanent</td>
      <td>Yes</td>
      <td>No</td>
    </tr>
  </table>

  <% unless @plist.empty? %>
    <ol>
    <% @plist.each do |plist| %>
      <li><%= plist.name %></li>
    <% end %>
  </ol>
  <% else%>
    <p>No person!</p>
  <% end %>

</div><!-- end list-view -->
